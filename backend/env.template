# Coral Rush Backend Configuration Template
# Copy this to .env and fill in your values

# Server Settings
PORT=4000
NODE_ENV=development
FRONTEND_URL=http://localhost:5173

# Swarms AI Configuration
SWARMS_API_URL=https://api.swarms.ai
SWARMS_API_KEY=sk_test_REPLACE_WITH_YOUR_SWARMS_KEY

# Solana Configuration
SOLANA_RPC_URL=https://api.devnet.solana.com
SOLANA_CLUSTER=devnet
SOLANA_EXPLORER_URL=https://explorer.solana.com
# Generate with: solana-keygen new --no-bip39-passphrase --silent --outfile keypair.json
# Then: cat keypair.json (copy the array format) OR solana-keygen pubkey keypair.json
SOLANA_LOGGING_KEYPAIR=your_base58_private_key_here_or_json_array

# Optional: Mistral AI (for fallback)
MISTRAL_API_KEY=your_mistral_api_key_if_needed
